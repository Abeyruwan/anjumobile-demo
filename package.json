  "dependencies": {
    "bcryptjs": "^3.0.2",
    "colors": "^1.4.0",
    "config": "^4.1.1",
    "express": "^5.1.0",
    "mongodb": "^6.18.0",
    "mongoose": "^8.18.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.1",
